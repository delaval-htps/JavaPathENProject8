<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="console" target="SYSTEM_OUT">
      <PatternLayout pattern="[%-17t] - %-5level  - %msg%n"/>
    </Console>
    <File name="FileAppender"  fileName="log/lastest-test.log" append="false" >
          <PatternLayout pattern="[%-17t] - %-5level  - %msg%n"/>
    </File>   
  </Appenders>
  <Loggers>
     <Root level="INFO" >
           <appender-ref ref="console" />
          <appender-ref ref="FileAppender" />
    </Root>

  </Loggers>
</Configuration>